#!/usr/bin/env lumo

(require 'yargs '[goog.string.format])

(def argv
  (-> yargs
      (.scriptName "hello")
      (.demandCommand 2)
      (.default "x" 10)
      (.command "oops")
      (.command "ouch")
      (.demandOption "y")
      .-argv
      (js->clj :keywordize-keys true)))

(prn argv)

(let [{:keys [x y]} argv]
  (println (goog.string.format "%d x %d is %d" x y (* x y))))
